<h1>Journal Articals</h1>
<table class='table table-striped'>
	<tr>
		<th>Journal Name</th>
		<th>Journal Artical Title</th>
		<th></th>
	</tr>
  <% JournalArticle.all.each do |journal| %>
    <tr>
			<td><%= journal.name %></td>
			<td><%= journal.article_title %></td>
			<td>
			<%= link_to 'View Journal Details', journal_article_path(journal), class: 'btn btn-primary' %>
			</td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to 'Add New Journal', new_journal_article_path, class: "btn" %>